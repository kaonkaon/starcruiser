            <string>//delete outside room
if(y &gt; room_height) instance_destroy(self);

//set bullet ai
if(ai = 0){
    image_blend = c_red;
    vspeed = 5;
}
else if(ai = 1){
    image_blend = c_olive;
    vspeed = 10;
}
else if(ai = 2){
    image_blend = c_yellow;
    vspeed = 5;
    x = clamp(x, osc_start, osc_end);
    if(x = osc_start) oscilate = 0;
    else if(x = osc_end) oscilate = 1;
    
    if(oscilate = 0) x += osc_speed;
    else if(oscilate = 1) x -= osc_speed;
}
else if(ai = 3){
    image_blend = c_lime;
    
    x = clamp(x, 32, room_width - 32);
    if(x = 32) sht_angle = 360 - (sht_angle - 180);
    else if(x = room_width - 32) sht_angle = 180 + (360 - sht_angle);
    
    direction = sht_angle;
    speed = 5;
}
else if(ai = 4){
    image_blend = c_fuchsia;
    
    vspeed += 0.2;
}
else if(ai = 5){
    image_blend = c_aqua;
    
    if(direction &gt; point_direction(x, y, obj_player.x, obj_player.y)) direction -= turn_speed;
    else if(direction &lt; point_direction(x, y, obj_player.x, obj_player.y)) direction += turn_speed;
    
    direction = clamp(direction, 180 + 30, 360 - 30)
    speed = 5;
}
